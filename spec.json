{
	"$schema": "../argspec/arguments.schema.json",
	"program": "tester",
	"description": "A tester program for programs which use parts of argspec",
	"version": "1.2.2",
	"date": "27/01/2019",
	"licence": "GPL+ asdfdfsafdafjkdslahfjkdsahflk",
	"bugs": "Please file any bugs or issues at <github.com/emperor-lang/argspec/issues>",
	"seeAlso": [
		{
			"manLocation": 1,
			"name": "gcc"
		},
		{
			"manLocation": 1,
			"name": "alex"
		},
		{
			"manLocation": 1,
			"name": "happy"
		}
	],
	"longDescription": "This program does not actually exist. But that doesn't mean that it can't be useful though. Perhaps in another life it will evolve in to something useful, but that certainly isn't this one. Anyway, thanks for reading this far, you just made my day :)",
	"author": "The author would like to remain anonymous or something",
	"args": [
		{
			"mandatory": false,
			"short": "-h",
			"long": "--help",
			"dest": "HELP",
			"type": "help",
			"default": "false",
			"help": "display a help message and exit"
		},
		{
			"mandatory": false,
			"short": "-*",
			"dest": "HELP_2",
			"type": "help",
			"default": "false",
			"help": "display a help message and exit"
		},
		{
			"mandatory": false,
			"dest": "verbose",
			"short": "-v",
			"long": "--verbose",
			"type": "string",
			"choices": [
				"0",
				"1",
				"2",
				"3"
			],
			"default": "Hello, world!",
			"help": "set output verbosity"
		},
		{
			"mandatory": true,
			"dest": "file",
			"short": "-i",
			"long": "--input",
			"type": "string",
			"default": "Hello, world!",
			"help": "set input file"
		},
		{
			"mandatory": true,
			"dest": "x",
			"long": "--something-only-long",
			"type": "int",
			"default": "0",
			"help": "set some X"
		},
		{
			"mandatory": true,
			"dest": "out",
			"short": "-o",
			"long": "--output-to",
			"type": "string",
			"default": "-",
			"help": "set output file"
		}
	]
}